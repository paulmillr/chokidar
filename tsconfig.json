{
  "extends": "@paulmillr/jsbt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": ".",
    "module": "node16",
    "moduleResolution": "node16",
    "allowSyntheticDefaultImports": true,
    "noImplicitReturns": false,
    "sourceMap": false,
    "declarationMap": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
